<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC 
	"-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" 
	"http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

	<application>
		<variable-resolver>
			org.springframework.web.jsf.DelegatingVariableResolver
		</variable-resolver>
	</application>
	<application>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>
	<navigation-rule>
		<navigation-case>
			<from-outcome>groupReport</from-outcome>
			<to-view-id>/groupReport.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>	
	<managed-bean>
		<description>Class comparison report that holds report beans</description>
		<managed-bean-name>classComparisonReport</managed-bean-name>
		<managed-bean-class>gov.nih.nci.eagle.web.reports.ClassComparisonReport</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>patientManager</property-name>
			<value>#{patientManager}</value>
		</managed-property>
		<managed-property>
			<property-name>findingsManager</property-name>
			<value>#{eagleFindingsManager}</value>
		</managed-property>
		<managed-property>
			<property-name>task</property-name>
			<value>#{sessionScope["task"]}</value>
		</managed-property>
		<managed-property>
			<property-name>groupReport</property-name>
			<value>#{groupReport}</value>
		</managed-property>
	</managed-bean>	
	<managed-bean>
		<description>FTest report that holds report beans</description>
		<managed-bean-name>ftestReport</managed-bean-name>
		<managed-bean-class>gov.nih.nci.eagle.web.reports.FTestReport</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>patientManager</property-name>
			<value>#{patientManager}</value>
		</managed-property>
		<managed-property>
			<property-name>findingsManager</property-name>
			<value>#{eagleFindingsManager}</value>
		</managed-property>
		<managed-property>
			<property-name>task</property-name>
			<value>#{sessionScope["task"]}</value>
		</managed-property>
		<managed-property>
			<property-name>groupReport</property-name>
			<value>#{groupReport}</value>
		</managed-property>		
	</managed-bean>	
	<managed-bean>
		<description>GLM report that holds report beans</description>
		<managed-bean-name>glmReport</managed-bean-name>
		<managed-bean-class>gov.nih.nci.eagle.web.reports.GLMReport</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>patientManager</property-name>
			<value>#{patientManager}</value>
		</managed-property>
		<managed-property>
			<property-name>findingsManager</property-name>
			<value>#{eagleFindingsManager}</value>
		</managed-property>
		<managed-property>
			<property-name>task</property-name>
			<value>#{sessionScope["task"]}</value>
		</managed-property>
		<managed-property>
			<property-name>groupReport</property-name>
			<value>#{groupReport}</value>
		</managed-property>		
	</managed-bean>	
	<managed-bean>
		<description>EPI Report</description>
		<managed-bean-name>epiReport</managed-bean-name>
		<managed-bean-class>gov.nih.nci.eagle.web.reports.EpiReport</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>findingsManager</property-name>
			<value>#{eagleFindingsManager}</value>
		</managed-property>
		<managed-property>
			<property-name>task</property-name>
			<value>#{sessionScope["task"]}</value>
		</managed-property>
	</managed-bean>	
	<managed-bean>
		<description>Report to show patient groups</description>
		<managed-bean-name>groupReport</managed-bean-name>
		<managed-bean-class>gov.nih.nci.eagle.web.reports.PatientGroupReport</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
</faces-config>
